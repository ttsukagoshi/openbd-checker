{
  "name": "openBD情報チェッカー",
  "version": "2.0.1",
  "description": "openBDに登録されている書誌・書影情報を、コンテクストメニューからISBNで検索・参照する。",
  "permissions": ["contextMenus", "storage"],
  "host_permissions": ["https://api.openbd.jp/"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/books16.png",
    "32": "images/books32.png",
    "48": "images/books48.png",
    "128": "images/books128.png"
  },
  "manifest_version": 3
}
